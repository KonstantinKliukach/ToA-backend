{
    "root": true,
    "parserOptions": {
         "ecmaVersion": "latest",
         "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "extends": ["eslint:recommended", "prettier", "plugin:@typescript-eslint/recommended",],
    "env": { 
         "es2021": true,
         "node": true
    },
    "rules": {
         "no-console": "error"
    }
}